@conference{Sears:2006,
	Annote = {Stasis is a storage framework built from a systems point of view -- it is meant to provide systems developers with the ability to build a storage apparatus that will best suit the needs of their particular system.  The authors argue that the relational model, though well-suited for various enterprise uses, is non-optimal for many current applications, such as scientific computing and search engines, which currently resort to ad-hoc methods rather than utilizing DBMS solutions.  In order to make Stasis more appropriate for such uses, the authors have created a systems-style, bottom-up layered framework that will enable good performance for various applications while still providing the database guarantees of atomicity and durability.  The authors adapt many of the algorithms from the ARIES paper.  Stasis also provides additional functionality, such as allowing user-defined operations and providing for LSN estimation to avoid having to store the LSN on each page, which is inconvenient for writing a data object that spans multiple pages.  The paper could have been more effective at supporting its initial claims if it provided a case study of how Stasis's extensible framework with custom operations could be used to better suit an application of the type mentioned in the introduction that motivated the development of such a nonmonolithic data store.
},
	Author = {Russell Sears and Eric Brewer},
	Date-Added = {2008-09-15 20:52:41 -0700},
	Date-Modified = {2008-09-15 20:56:19 -0700},
	Organization = {USENIX, The Advanced Computing Systems Association},
	Pages = {29-44},
	Series = {OSDI},
	Title = {Stasis: Flexible Transactional Storage},
	Year = {2006}}

@article{Mohan:1992,
	Annote = {The authors present ARIES, an algorithm for failure recovery in the setting of write-ahead logging (WAL).  Some of the key features of their algorithm are support for partial rollbacks and fine-granularity locking.  The paper fills in some holes in the literature, such as open questions related to fine-granularity locking.  The authors present a set of goals for their system, including simplicity, operation logging, and partial rollbacks.  They mention that some of the goals are in conflict with each other and that they make tradeoffs among the goals; however, they do not spend time providing further explanation of what the conflicts are and what tradeoffs are made.  Given that so many goals were listed, this would have been helpful.  A key emphasis of the paper was in pointing out that some of the previously attempted methods did not handle restart processing correctly with respect to the ordering and implementation of the redo and undo passes.  The authors argue for the ordering of a full redo pass before the undo pass (System R reverses the execution of these passes).  They also mention that many systems perform selective redo (redo only of in-doubt transactions), which is not compatible with WAL.  ARIES instead does full redo, which they refer to as repeating history.  In the conclusion, however, the authors mention that they extended ARIES so that repeating history would be lessened for loser transactions.  This seems to contradict their earlier statements about full redo being of greatest importance.
},
	Author = {C. Mohan and Don Haderle and Bruce Lindsay and Hamid Pirahesh and Peter Schwarz},
	Date-Added = {2008-09-13 16:22:21 -0700},
	Date-Modified = {2012-05-08 11:23:48 -0700},
	Journal = {ACM Transactions on Database Systems},
	Keywords = {Algorithms, Design, Performance, Reliability, Buffer management, latching, locking, space management, write-ahead logging},
	Month = {March},
	Number = {1},
	Pages = {94-162},
	Title = {ARIES: A Transaction Recovery Method Supporting Fine-Granularity Locking and Partial Rollbacks Using Write-Ahead Logging},
	Volume = {17},
	Year = {1992},
	Bdsk-File-1 = {}}

@article{Wilkes:1996,
	Annote = {Researchers at HP devised and implemented an automatic RAID controller.  It makes use of a two-level storage hierarchy, consisting of mirrored storage and RAID 5 storage.  Based on the current workload, the controller shifts data in block form between the two levels of the hierarchy.  The controller is able to handle diverse workloads.  It can also adapt to changes to the disk array, such as addition/removal of disks.  Thus, a very useful consequence is that it can be used to migrate data from one disk array to another, by adding/removing one disk at a time.  The controller will gradually balance the data over the augmented disk array so that the disk array can eventually be totally replaced without ever having to stop the system running on top of it.

Early in the paper, the authors commented that a major difficulty in array configuration is adapting to diverse workloads.  In their simulation studies, they mentioned that they played back traces collected from various systems.  However, in the conclusion, the authors mentioned that there are some workloads for which their algorithms would not work well.  They could have looked into the features of a workload that would enable high performance and likewise for workloads for which low performance was observed.  More energy looking into the case where performance was poor would give more insight that would help for refining their algorithms, even to the point of adapting to the characteristics of the current workload.},
	Author = {John Wilkes and Richard Golding and Carl Staelin and Tim Sullivan},
	Date-Added = {2008-09-09 10:23:57 -0700},
	Date-Modified = {2012-05-14 15:23:32 -0700},
	Journal = {ACM Transactions on Computer Systems},
	Keywords = {Algorithms, Design, Performance, Reliability, Disk array, RAID, storage hierarchy},
	Month = {February},
	Number = {1},
	Pages = {108-136},
	Title = {The HP AutoRAID Hierarchical Storage System},
	Volume = {14},
	Year = {1996}}

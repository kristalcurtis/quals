	Booktitle = {ACM Special Interest Group on Performance Evaluation (SIGMETRICS)},
	Date-Added = {2009-08-13 11:02:14 -0700},
	Date-Modified = {2010-08-13 11:27:57 -0700},
	Title = {An {A}nalytical {M}odel for {M}ulti-tier {I}nternet {S}ervices and {I}ts {A}pplications},
